@model Atos.Web.Mobile.Trends.Models.User

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>User</legend>

    <div class="display-label">Gender</div>
    <div class="display-field">@Model.Gender</div>

    <div class="display-label">Region</div>
    <div class="display-field">@Model.Region</div>

    <div class="display-label">Browser</div>
    <div class="display-field">@Model.Browser</div>

    <div class="display-label">BrowserVersion</div>
    <div class="display-field">@Model.BrowserVersion</div>

    <div class="display-label">Platform</div>
    <div class="display-field">@Model.Platform</div>

    <div class="display-label">IPAddress</div>
    <div class="display-field">@Model.IPAddress</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


