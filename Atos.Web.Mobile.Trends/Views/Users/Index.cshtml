@model IEnumerable<Atos.Web.Mobile.Trends.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Gender
        </th>
        <th>
            Region
        </th>
        <th>
            Browser
        </th>
        <th>
            BrowserVersion
        </th>
        <th>
            Platform
        </th>
        <th>
            IPAddress
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
			@item.Gender
        </td>
        <td>
			@item.Region
        </td>
        <td>
			@item.Browser
        </td>
        <td>
			@item.BrowserVersion
        </td>
        <td>
			@item.Platform
        </td>
        <td>
			@item.IPAddress
        </td>
    </tr>
}

</table>


