@model Atos.Web.Mobile.Trends.Models.Browser

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Browser</legend>

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">Version</div>
    <div class="display-field">@Model.Version</div>

    <div class="display-label">Platform</div>
    <div class="display-field">@Model.Platform</div>

    <div class="display-label">ClientIP</div>
    <div class="display-field">@Model.ClientIP</div>

    <div class="display-label">TimeStamp</div>
    <div class="display-field">@String.Format("{0:g}", Model.TimeStamp)</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>


